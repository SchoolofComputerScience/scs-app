{
  "name": "scs-app",
  "description": "CMU:SCS Main Application",
  "author": "seckel@scs.cmu.edu",
  "version": "2.0.0",
  "repository": "https://github.com/SchoolofComputerScience/scs-app",
  "private": true,
  "engines": {
    "node": "8.2.1",
    "npm": ">=5.0"
  },
  "scripts": {
    "dev": "node server",
    "start": "node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "unit-test": "cross-env NODE_ENV=test node_modules/karma/bin/karma start test/unit/karma.conf.js --single-run=false",
    "test": "cross-env NODE_ENV=test node_modules/karma/bin/karma start test/unit/karma.conf.js --reporter progress"
  },
  "dependencies": {
    "apollo-client": "^1.9.0-0",
    "compression": "^1.7.0",
    "cors": "^2.8.3",
    "date-fns": "^1.28.5",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.15.3",
    "express-robots": "^0.1.6",
    "graphql-tag": "^2.4.2",
    "helmet": "^3.6.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "marked": "^0.3.12",
    "mongoose": "^4.11.1",
    "node-noop": "^1.0.0",
    "prismic.io": "^3.5.7",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.3",
    "vue": "^2.3.4",
    "vue-analytics": "^4.1.2",
    "vue-observe-visibility": "^0.1.3",
    "vue-resize": "^0.1.3",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.3.4",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.5",
