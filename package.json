    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-webpack": "^2.0.9",
    "modularscale-sass": "^3.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
